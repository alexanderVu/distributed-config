{
  "name": "distributed-config",
  "version": "0.9.3",
  "description": "Load distributed config files on node js projects, with ability to extend or override settings throw environment variables.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./ --fix",
    "pretest": "NODE_ENV=test",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:fix": "npm run lint && jest --coverage",
    "jsdoc": "rimraf ./docs/api && jsdoc --readme ./README.md -c .jsdoc.json && opn ./docs/api/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexanderVu/distributed-config.git"
  },
  "engines": {
    "node": ">10.0.0",
    "npm": ">6.0.0"
  },
  "keywords": [
    "conf",
    "config",
    "configuration",
    "load",
    "env",
    "environment",
    "distributed"
  ],
  "author": {
    "name": "Alexander Vu",
    "email": "alexander.vu.tuyet@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "codecov": "^3.1.0",
    "docdash": "^1.0.2",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.0.0",
    "jsdoc": "^3.5.5",
    "mock-fs": "^4.7.0",
    "opn-cli": "^4.0.0",
    "rimraf": "^2.6.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "dependencies": {
    "flat": "^4.1.0",
    "hjson": "^3.1.2",
    "is-obj": "^1.0.1",
    "js-yaml": "^3.12.1",
    "json5": "^1.0.1",
    "dot-prop-opt": "^1.0.1"
  }
}
